{
  "name": "taai-web",
  "version": "1.0.0",
  "description": "Technical Analysis Artificial intelligence",
  "license": "MIT",
  "keywords": [
    "stock",
    "technical analysis",
    "artificial intelligence",
    "lstm"
  ],
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@mui/styles": "^5.13.2",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "d3-dsv": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "moment": "^2.29.4",
    "qrcode": "^1.5.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-financial-charts": "^1.3.2",
    "react-hook-form": "^7.44.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.2",
    "react-virtualized-auto-sizer": "^1.0.7",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@types/d3-dsv": "^2.0.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "react-error-overlay": "^6.0.9",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
